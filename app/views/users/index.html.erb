<div class="container">
  <% provide :title, t("all_users") %>
  <h1><%= t "users.index.all_users" %></h1>
  <ul class="users">
    <% @users.each do |user| %>
      <li>
        <% unless user.is_admin? %>
          <%= link_to user.name, user %>
        <% end %>
      </li>
    <% end %>
  </ul>
  <%= will_paginate %>
</div>
