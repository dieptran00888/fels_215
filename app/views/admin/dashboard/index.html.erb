<% provide :title, t(".admin") %>
<%= render "layouts/admin/admin_navbar" %>
<div class="col-md-9">
    <table class="table table-striped tbl-lessons panel-default ">
      <thead>
        <tr class="panel-heading">
          <th><%= "User" %></th>
          <th><%= t ".created_at" %></th>
          <th><%= t "name" %></th>
          <th><%= t ".status" %></th>
          <th><%= t "duration" %></th>
          <th><%= t "word_per_lesson" %></th>
          <th><%= t ".spent_time" %></th>
          <th><%= t ".score" %></th>
          <th></th>
        </tr>
      </thead>
      <tbody class="list-lesson panel-body">
        <%= render "admin/dashboard/lesson", lessons: @lessons %>
      </tbody>
    </table>

</div>